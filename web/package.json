{
  "name": "vue-app",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --skipLibCheck",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^14.2.1",
    "consola": "^3.4.2",
    "pinia": "^3.0.4",
    "seedrandom": "^3.0.5",
    "vue": "^3.5.13",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@types/node": "^25.2.2",
    "@types/seedrandom": "^3.0.8",
    "@vitejs/plugin-vue": "^5.2.4",
    "@xiaowaibuzheng/rolldown-vite-split-chunks": "^0.0.19",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^1.4.0",
    "lint-staged": "^15.2.11",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unocss": "^66.6.0",
    "vite": "8.0.0-beta.13",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
